
<launch>

  <node name="laser_filter"  pkg="turtlebot_navigation" type="laser_footprint_filter" />
  <!--启动gmaping-->
  <arg name="custom_gmapping_launch_file" default="$(find turtlebot_navigation)/launch/includes/gmapping/gmapping.launch.xml"/>
  <include file="$(arg custom_gmapping_launch_file)">
  <arg name="scan_topic" value="scan_filtered" />
  </include>

  <!-- Move base -->
  <include file="$(find fare_well)/launch/move_base.launch.xml">
  <arg name="laser_topic" value="scan_filtered" />
  <arg name="type" value="gmapping"/>
  </include>


  <!--启动虚拟辅助定位-->
  <node pkg="fake_localization" type="fake_localization" name="fake_localization" output="screen" />
    <!-- 启动rviz -->
    <include file="$(find turtlebot_rviz_launchers)/launch/view_navigation.launch"/>

</launch>
